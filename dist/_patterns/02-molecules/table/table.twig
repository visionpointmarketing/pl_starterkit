<div class="table-wrapper{% if table_class %} {{ table_class }}{% endif %}">
	<table>
		{% if table_head %}
		<thead>
			<tr>
			{% for row in table_head %}
				<td>{{ row|raw }}</td>
			{% endfor %}
			</tr>
		</thead>
		{% endif %}
		<tbody>
		{% for row in table_rows %}
			<tr>
				{% for cell in row %}
					<td>{{ cell|raw }}</td>
				{% endfor %}
			</tr>
		{% endfor %}
		</tbody>
	</table>
</div>